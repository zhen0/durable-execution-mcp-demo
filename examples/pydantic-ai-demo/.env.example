# FastMCP Cloud - URL of your deployed Prefect MCP server
# Get this from FastMCP Cloud after deploying the prefect-mcp-server
FASTMCP_SERVER_URL=https://your-server-name.fastmcp.app/mcp

# Model Provider API Key - choose one (or both if you want flexibility)
# For Anthropic Claude models (default):
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# For OpenAI models (optional):
# OPENAI_API_KEY=sk-your-openai-api-key-here

# Model to use (optional, defaults to Claude 3.5 Sonnet)
# Examples:
#   PYDANTIC_AI_MODEL=anthropic:claude-3-5-sonnet-20241022
#   PYDANTIC_AI_MODEL=anthropic:claude-3-opus-20240229
#   PYDANTIC_AI_MODEL=openai:gpt-4o
#   PYDANTIC_AI_MODEL=openai:gpt-4o-mini
PYDANTIC_AI_MODEL=anthropic:claude-3-5-sonnet-20241022

# Prefect Cloud credentials (for deploying)
# Get these from https://app.prefect.cloud
PREFECT_API_URL=https://api.prefect.cloud/api/accounts/[ACCOUNT_ID]/workspaces/[WORKSPACE_ID]
PREFECT_API_KEY=pnu_your-prefect-api-key-here
